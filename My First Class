public class Main {
    public static void main(String[] args) {

        Student s1 = new Student();

        s1.name = "Vikas";
        s1.age = 25;
        s1.rollNumber = 101;

        Student s2 = new Student();

        s2.name = "Tanu";
        s2.age = 22;
        s2.rollNumber = 20;


        s1.displayInfo();
        s2.displayInfo();
    }
}


class student 

public class Student {

    String name;
    int age;
    int rollNumber;

    void displayInfo() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
        System.out.println("Roll Number: " + rollNumber);
    }
}
